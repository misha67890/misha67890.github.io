    help: ['Pomoc przy znacznikach'],
    mode: [0, -340, 'Tryb wiki-mark up', 'Wizualny tryb edycji'],
    lang: {
      too_long: 'Strona zbyt d&#322;uga. Prosz&#281; j&#261; skr&#243;ci&#263;.'
    },
    onPlainToggle: function(on, btn) {
      if (btn) {
        ajax.post('al_pages.php', {act: 'toggle_plain', state: on ? 1 : 0}, {})
      }
      if (on) {
        show('wk_wiki_help');
      } else {
        hide('wk_wiki_help');
      }
    }
  });
  cur.editors.push(editor);
